{
  "name": "pianosight",
  "version": "1.0.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "eslint": "^7.5.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "nollup": "^0.12.0",
    "postcss-import": "^12.0.1",
    "rollup": "^2.10.2",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-workbox-inject": "^2.0.0",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "opensheetmusicdisplay": "^0.8.2",
    "register-service-worker": "^1.7.1",
    "sirv-cli": "^0.4.4",
    "svg-piano": "^0.3.1",
    "webmidi": "^2.5.1"
  },
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "sw": "rollup -c --environment SERVICE_WORKER",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "rollup -c -w --environment LIVERELOAD",
    "dev:nollup": "nollup -c --environment NOLLUP --hot --port 5000 --content-base ./public",
    "dev": "npm run dev:nollup",
    "lint": "eslint src/*.js src/*.svelte",
    "icon": "rsvg-convert -b white -w 512 public/favicon.svg -o public/favicon-512.png; rsvg-convert -b white -w 192 public/favicon.svg -o public/favicon-192.png; pngquant -f --ext .png 2 public/favicon-512.png public/favicon-192.png",
    "start": "sirv public"
  }
}
